(define-enum GPSDControlMethod
  (in-module "Location")
  (c-name "LocationGPSDControlMethod")
  (gtype-id "LOCATION_TYPE_GPSD_CONTROL_METHOD")
  (values
    '("user-selected" "LOCATION_METHOD_USER_SELECTED")
    '("cwp" "LOCATION_METHOD_CWP")
    '("acwp" "LOCATION_METHOD_ACWP")
    '("gnss" "LOCATION_METHOD_GNSS")
    '("agnss" "LOCATION_METHOD_AGNSS")
  )
)

