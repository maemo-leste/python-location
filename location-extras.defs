     '("speed" "LOCATION_GPS_DEVICE_SPEED_SET")
     '("track" "LOCATION_GPS_DEVICE_TRACK_SET")
     '("climb" "LOCATION_GPS_DEVICE_CLIMB_SET")
     '("latlong" "LOCATION_GPS_DEVICE_LATLONG_SET")
     '("time" "LOCATION_GPS_DEVICE_TIME_SET")
   )
)
